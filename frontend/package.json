{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "cross-env DEV=true vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "npm run test:unit && npm run test:integration && npm run test:e2e",
		"test:unit": "vitest run --reporter=verbose",
		"test:unit:watch": "vitest",
		"test:integration": "vitest run tests/integration --reporter=verbose",
		"test:integration:watch": "vitest tests/integration",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:headed": "playwright test --headed",
		"test:components": "vitest run src/lib/components/__tests__",
		"test:api": "vitest run tests/integration",
		"test:setup": "node scripts/setup-test-data.js",
		"test:clean": "node scripts/clean-test-data.js",
		"test:emulator": "firebase emulators:start --only auth,firestore",
		"create-admin": "node scripts/create-admin.js"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@playwright/test": "^1.49.1",
		"@skeletonlabs/skeleton": "^3.2.0",
		"@skeletonlabs/skeleton-svelte": "^1.5.1",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/postcss": "^4.0.0",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/coverage-v8": "^1.6.0",
		"autoprefixer": "^10.4.21",
		"axios": "^1.11.0",
		"cross-env": "^10.1.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^24.1.0",
		"lucide-svelte": "^0.541.0",
		"postcss": "^8.5.6",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^5.2.6",
		"@lucide/svelte": "^0.541.0",
		"@mux/mux-node": "^12.8.0",
		"@sendgrid/mail": "^8.1.5",
		"@stripe/stripe-js": "^7.9.0",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@types/marked": "^5.0.2",
		"algoliasearch": "^5.35.0",
		"canvas-confetti": "^1.9.4",
		"dotenv": "^17.2.3",
		"driver.js": "^1.3.6",
		"firebase": "^12.1.0",
		"firebase-admin": "^13.4.0",
		"g": "^2.0.1",
		"hls.js": "^1.6.13",
		"marked": "^16.4.1",
		"node-fetch": "^3.3.2",
		"shepherd.js": "^14.5.1",
		"stripe": "^18.5.0",
		"vercel": "^47.1.4"
	}
}
